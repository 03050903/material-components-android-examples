<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="colorScrim" format="color" />
    <attr name="colorStatusBar" format="color" />

    <attr name="bottomNavigationDrawerStyle" format="reference" />
    <attr name="navigationViewTheme" format="reference" />

    <attr name="foregroundShapeAppearance" format="reference" />

    <declare-styleable name="BottomNavigationDrawer">
